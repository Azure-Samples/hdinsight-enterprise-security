{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "clusterName": {
            "value": "<CLUSTERNAME>"
        },
        "clusterLoginUserName": {
            "value": "admin"
        },
        "clusterLoginPassword": {
            "value": "<PASSWORD>"
        },
        "location": {
            "value": "eastus"
        },
        "clusterWorkerNodeCount": {
            "value": 4
        },
        "clusterKind": {
            "value": "SPARK"
        },
        "clusterVersion": {
            "value": "3.6"
        },
        "sshUserName": {
            "value": "sshuser"
        },
        "sshPassword": {
            "value": "<PASSWORD>"
        },
        "minTlsVersionNumber": {
            "value": "1.2"
        },
        "publicNetworkAccess": {
            "value": "OutboundOnly"
        },
        "outboundOnlyPublicNetworkAccessType": {
            "value": "PublicLoadBalancer"
        },
        "isEncryptionInTransitEnabled": {
            "value": true
        }
    }
}
